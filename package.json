{
  "name": "trellocode",
  "displayName": "TrelloCode",
  "description": "Connect Trello to Code.",
  "version": "0.0.1",
  "publisher": "JohnAaronNelson",
  "engines": {
    "vscode": "^1.25.0"
  },
  "license": "MIT",
  "homepage": "https://github.com/celador/vscode-trello/blob/master/README.md",
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/celador/vscode-trello"
  },
  "keywords": [
    "Trello",
    "Tracking",
    "Project Organization"
  ],
  "icon": "taco.png",
  "galleryBanner": {
    "color": "#A1A0A0",
    "theme": "light"
  },
  "bugs": {
    "url": "https://github.com/celador/vscode-trello/issues"
  },
  "activationEvents": [
    "onCommand:extension.loginToTrello",
    "onCommand:extension.moveCardToNewList",
    "onCommand:extension.getAllBoards",
    "onCommand:extension.closeCard"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [{
        "command": "extension.moveCardToNewList",
        "title": "Trello: Move Current Card to New List"
      },
      {
        "command": "extension.loginToTrello",
        "title": "Trello: Login"
      },
      {
        "command": "extension.getAllBoards",
        "title": "Trello: Get A Card"
      },
      {
        "command": "extension.closeCard",
        "title": "Trello: Close Current Card"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "TrelloCode configuration",
      "properties": {
        "TrelloCode.apiKey": {
          "type": ["string", "null"],
          "default": null,
          "description": "Your Trello API key from https://trello.com/app-key"
        },
        "TrelloCode.userToken": {
          "type": ["string", "null"],
          "default": null,
          "description": "The User Token returned when logging into Trello"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.6.1",
    "vscode": "^1.1.6",
    "tslint": "^5.8.0",
    "@types/node": "^7.0.43",
    "@types/mocha": "^2.2.42",
    "@types/open": "0.0.29"
  },
  "dependencies": {
    "node-trello": "^1.3.0",
    "open": "0.0.5",
    "trello": "^0.9.0"
  }
}